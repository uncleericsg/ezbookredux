import * as React from 'react';

export interface RateLimitSettings;

export interface ChatGPTSettings;

export interface FCMSettings;

export interface StripeSettings;

export interface RepairShoprSettings;

export interface CypressSettings;

export interface AppSettings;


/**
 * Rate limit settings for APIs.
 */

  /**
   * Time window in milliseconds for rate limiting.
   */
  windowMs: number;
}

/**
 * ChatGPT API settings.
 */

  /**
   * Model to use for ChatGPT.
   */
  model: string;
  /**
   * Whether ChatGPT is enabled.
   */
  enabled: boolean;
  /**
   * Maximum number of tokens to generate.
   */
  maxTokens: number;
  /**
   * Temperature for ChatGPT responses.
   */
  temperature: number;
  /**
   * Rate limit settings for ChatGPT API.
   */
  rateLimit: RateLimitSettings;
}

/**
 * Firebase Cloud Messaging (FCM) settings.
 */

  /**
   * Maximum number of tokens to generate.
   */
  maxTokens: number;
  /**
   * Rate limit settings for FCM API.
   */
  rateLimit: RateLimitSettings;
}

/**
 * Stripe payment settings.
 */

  /**
   * Secret key for Stripe.
   */
  secretKey: string;
  /**
   * Whether Stripe is enabled.
   */
  enabled: boolean;
  /**
   * Webhook secret for Stripe.
   */
  webhookSecret?: string;
}

/**
 * RepairShopr API settings.
 */

  /**
   * Whether RepairShopr is enabled.
   */
  enabled: boolean;
  /**
   * Subdomain for RepairShopr.
   */
  subdomain?: string;
}

/**
 * Cypress testing settings.
 */

  /**
   * Whether Cypress is enabled.
   */
  enabled: boolean;
  /**
   * Project ID for Cypress.
   */
  projectId?: string;
}

/**
 * Application settings.
 */

  /**
   * RepairShopr API settings.
   */
  repairShopr?: RepairShoprSettings;
  /**
   * Stripe payment settings.
   */
  stripe?: StripeSettings;
  /**
   * ChatGPT API settings.
   */
  chatGPT?: ChatGPTSettings;
  /**
   * Cypress testing settings.
   */
  cypress?: CypressSettings;
  /**
   * FCM settings.
   */
  fcm?: FCMSettings;
  /**
   * Theme for the application.
   */
  theme?: 'light' | 'dark' | 'system';
  /**
   * Language for the application.
   */
  language?: string;
  /**
   * Timezone for the application.
   */
  timezone?: string;
  /**
   * Date format for the application.
   */
  dateFormat?: string;
  /**
   * Notification settings.
   */
  notifications?: {
    /**
     * Whether email notifications are enabled.
     */
    email: boolean;
    /**
     * Whether push notifications are enabled.
     */
    push: boolean;
    /**
     * Whether SMS notifications are enabled.
     */
    sms: boolean;
  };
}

/**
 * Default application settings.
 */
export const defaultAppSettings: AppSettings = {
  loginScreenEnabled: true,
  theme: 'system',
  language: 'en',
  timezone: 'UTC',
  dateFormat: 'YYYY-MM-DD',
  notifications: {
    email: true,
    push: true,
    sms: false,
  },
};

undefined.displayName = 'undefined';